$(".plq-listing").html("");
<% if @vendors.any? %> 
  <% @vendors.each do |vendor| %>
    $(".plq-listing").append("<%= escape_javascript(render 'vendor_item', user: vendor) %>");
  <% end %>

<% else %>
  $(".plq-listing").html("Currently we are not active in your region. Be the first one to subscribe as an agent / vendor.");
<% end %>